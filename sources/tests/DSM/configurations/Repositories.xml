<?xml version="1.0" encoding="UTF-8"?>
<repositories>
    <repository type="Daemon" name="" unique_identifier="" link="">
        <dameons>
            <daemon name="DSM Daemon"
                    unique_identifier=""
                    library="DSMDaemon.jar"
                    namespace="dsm.daemon.dsmdaemon"
                    class_name="DSMDaemon"
                    configuration_file_full_path="DSMDaemonConfigurationFile.xml" />
        </dameons>
    </repository>

    <repository type="ActionsManager" name="" unique_identifier="" link="">
        <actions_managers>
            <actions_manager name="DSM Simple Action Manager"
                    unique_identifier=""
                    library="DSMSimpleActionManager.jar"
                    namespace="dsm.action.manager"
                    class_name="SimpleActionsManager"
                    configuration_file_full_path="SimpleActionsManager.xml" />
        </actions_managers>
    </repository>

    <repository type="Actions" name="" unique_identifier="" link="">
        <actions>
            <action library="SimpleAtomicActions.jar"
                    namespace="dsm.action.atomic.simpleactions"
                    class_name="LocalTextFileReader" />

            <action library="SimpleAtomicActions.jar"
                    namespace="dsm.action.atomic.simpleactions"
                    class_name="LocalTextFileWriter" />

            <action library="SimpleDistributedActions.jar"
                    namespace="dsm.action.distributed.simpleactions"
                    class_name="NetworkTextFileReader" />

            <action library="SimpleDistributedActions.jar"
                    namespace="dsm.action.distributed.simpleactions"
                    class_name="NetworkTextFileWriter" />

            <action library="SimpleDistributedActions.jar"
                    namespace="dsm.action.distributed.simpleactions"
                    class_name="TextFileCopy" />
        </actions>
    </repository>

</repositories>
